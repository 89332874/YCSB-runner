# Configuration from YCSB+T paper
recordcount=10000
operationcount=1000000
workload=com.yahoo.ycsb.workloads.ClosedEconomyWorkload
totalcash=10000000

readproportion=0.9
updateproportion=0
insertproportion=0
scanproportion=0
readmodifywriteproportion=0.1
requestdistribution=zipfian

fieldcount=1
fieldlength=100

writeallfields=true
# running on JDBC with readallfields=true causes exceptions
readallfields=false

# Statistics collection
histogram.buckets=0

# PostgreSQL Configuration
db.driver=com.mysql.jdbc.Driver
db.url=jdbc:postgresql://localhost:5432/ycsb
db.user=ycsb
db.passwd=ycsb
jdbc.isolation=read_uncommitted
